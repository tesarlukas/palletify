{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "es5",
    "lib": [
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true, // HERE
